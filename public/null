use App\Models\Student;
use App\Models\Teacher;
use App\Models\AcademicYear;
use App\Models\Grade;
use App\Models\Subject;
use App\Models\StudentGrade;
use App\Models\TeacherSubject;
use App\Models\DataStudent;
use App\Models\DataTeacher;
use App\Models\Competency;
use App\Models\StudentCompetency;
use Illuminate\Support\Facades\DB;

StudentCompetency::join('competencies', 'competencies.id', '=', 'student_competencies.competency_id')->select('*', DB::raw('CONCAT(score, passing_grade)'))->get();
  
















use App\Models\Student;
use App\Models\Teacher;
use App\Models\AcademicYear;
use App\Models\Grade;
use App\Models\Subject;
use App\Models\StudentGrade;
use App\Models\TeacherSubject;
use App\Models\DataStudent;
use App\Models\DataTeacher;
use App\Models\Competency;
use App\Models\StudentCompetency;
use Illuminate\Support\Facades\DB;

StudentCompetency::join('competencies', 'competencies.id', '=', 'student_competencies.competency_id')->select('*', DB::raw('WHEN score >= passing_grade THEN "lulus"'))->get();
  
















